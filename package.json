{
  "name": "dc-portal",
  "version": "1.0.0",
  "description": "DC-Portal UI based on Bootstrap and REACT",
  "private": true,
  "scripts": {
    "start": "set PORT=3011 && react-scripts start",
    "build": "set PUBLIC_URL=v1.2beta/assets&& react-scripts build",
    "test": "nyc cross-env NODE_ENV=test jasmine",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "jshint": "jshint src/",
    "eject": "react-scripts eject"
  },
  "author": "Shubham Paskanti",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "axios": "0.21.1",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "highcharts": "9.2.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "react": "17.0.2",
    "react-bootstrap": "1.6.1",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "uuid": "8.3.2",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-async-to-generator": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.15.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "bower": "1.8.8",
    "braces": "3.0.2",
    "cross-env": "6.0.0",
    "css-loader": "6.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "fetch-mock": "9.0.0-beta.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "ignore-styles": "5.0.1",
    "jasmine": "3.5.0",
    "jasmine-enzyme": "7.1.1",
    "jsdom": "16.4.0",
    "jshint": "2.11.1",
    "kind-of": "6.0.3",
    "mini-css-extract-plugin": "2.2.0",
    "minimist": "1.2.5",
    "node-fetch": "2.6.1",
    "node-sass": "6.0.1",
    "nyc": "14.1.1",
    "postcss": "8.3.6",
    "postcss-loader": "6.1.1",
    "protractor": "5.4.2",
    "react-test-renderer": "16.9.0",
    "webpack": "4.44.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": "off",
      "no-unused-vars": "off",
      "no-useless-constructor": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/role-supports-aria-props": "off",
      "react/jsx-no-target-blank": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
